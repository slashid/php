{
    "name": "zsanmartin/slashid-php",
    "description": "PHP SDK for integrating with SlashID API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SlashId\\Php\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jos√© San Martin",
            "email": "jose@josesanmartin.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.50"
    },
    "scripts": {
        "test-cs": "vendor/bin/php-cs-fixer check --rules=@Symfony,@PER-CS2.0 src/",
        "test-cs-fix": "vendor/bin/php-cs-fixer fix --rules=@Symfony,@PER-CS2.0 src/"
    }
}
